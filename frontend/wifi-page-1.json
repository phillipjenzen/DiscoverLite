{
    "metadata": {
        "version": "2.0"
    },
    "contentContainerWidth": "narrow",
    "content": [
        {
            "elementType": "divider",
            "borderColor": "transparent"
        },
        {
            "elementType": "container",
            "id": "simple_content_table",
            "content": [
                {
                    "elementType": "blockHeading",
                    "heading": "Setting Up An Eduroam Wireless Connection By Device:",
                    "headingLevel": 1
                }
            ]
        },
        {
            "elementType": "container",
            "id": "disclosure_icon",
            "content": [
                {
                    "elementType": "collapsible",
                    "disclosureIcon": "chevron",
                    "title": "Android Device",
                    "image": {
                        "url": "https://1000logos.net/wp-content/uploads/2016/10/Android-Logo-2008.png",
                        "alt": "Android"
                    },
                    "content": [
                        {
                            "elementType": "table",
                            "id": "borders",
                            "outerBorderWidth": "1px",
                            "rowBorderStyle": "solid",
                            "columnBorderStyle": "none",
                            "rowHeaderBorderStyle": "solid",
                            "rowHeaderBorderWidth": "1px",
                            "padding": "xtight",
                            "rows": [
                                {
                                    "cells": [
                                        {
                                            "title": "1. Click on the Settings app."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "2. Click on Wi-Fi."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "3. Select Eduroam."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "4. Select Connect to Network."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "5. Select TTLS for the EAP Method."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "6. Select PAP for the Phase 2 Authentication."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "7. Enter your full email address in the Identity field. <br> (username@mail.fresnostate.edu)"
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "8. Enter your email password in the Password field."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "9. Click on Save."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "10. You will get a confirmation screen showing that you are connected to Eduroam."
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "elementType": "collapsible",
                    "disclosureIcon": "chevron",
                    "title": "Apple Device",
                    "image": {
                        "url": "https://cdn.mos.cms.futurecdn.net/6bTF6C2QiWXvhi33fJi3AC-1920-80.jpg.webp",
                        "alt": "Apple"
                    },
                    "content": [
                        {
                            "elementType": "table",
                            "id": "borders",
                            "outerBorderWidth": "1px",
                            "rowBorderStyle": "solid",
                            "columnBorderStyle": "none",
                            "rowHeaderBorderStyle": "solid",
                            "rowHeaderBorderWidth": "1px",
                            "padding": "xtight",
                            "rows": [
                                {
                                    "cells": [
                                        {
                                            "title": "1. Click on Settings."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "2. Click on Wi-Fi."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "3. Select Bulldogs."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "4. Open the Safari browser."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "5. Select \"If you need to configure your device for Eduroam - Click here.\". <br> If you are not redirected to the appropriate site, click on the direct install link: https://cloud.securew2.com/public/78509/eduroam/"
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "6. Enter your full email address. <br> (username@mail.fresnostate.edu)"
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "7. Make sure your iOS is selected, then click on JoinNow."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "8. Click on Install when you are prompted to install the new profile."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "9. Enter your mobile device password and click on Install."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "10. Enter your email password and click on Next, then Done."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "11. You will get a window with instructions to select the Eduroam Wi-Fi network from the Settings app."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "12. The small check mark next to Eduroam indicates that you are connected."
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "elementType": "collapsible",
                    "disclosureIcon": "chevron",
                    "title": "Windows Device",
                    "image": {
                        "url": "https://blogs.windows.com/wp-content/uploads/sites/2/2012/02/6874.5_5F00_01C91EBC.png",
                        "alt": "Windows"
                    },
                    "content": [
                        {
                            "elementType": "table",
                            "id": "borders",
                            "outerBorderWidth": "1px",
                            "rowBorderStyle": "solid",
                            "columnBorderStyle": "none",
                            "rowHeaderBorderStyle": "solid",
                            "rowHeaderBorderWidth": "1px",
                            "padding": "xtight",
                            "rows": [
                                {
                                    "cells": [
                                        {
                                            "title": "1. Click on your device network icon."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "2. Click on connect."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "3. In the \"username\" box enter your full Fresno State Email. <br> (username@mail.fresnostate.edu)"
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "4. In the \"Password\" box enter your Fresno State password."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "5. Click \"Enter\"."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "6. Wait and you may be prompted with a warning, skip and click connect anyways."
                                        }
                                    ]
                                },
                                {
                                    "cells": [
                                        {
                                            "title": "7. You are now connected to Eduroam."
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}
